    RowBox[{
     RowBox[{
     "it", " ", "is", " ", "the", " ", "waveform", " ", "of", " ", "the", " ",
       "transmission", " ", "line", " ", "as", " ", "a", " ", "function", " ",
       "of", " ", "t", " ", "at", " ", "z"}], "=", "0."}]}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"V\[LetterSpace]plus\[LetterSpace]line", "[", 
    RowBox[{
    "n_", ",", "z_", ",", "t_", ",", "vp_", ",", "Z0_", ",", "RS_", ",", 
     "RL_", ",", "L_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"H", "[", "z", "]"}], 
      RowBox[{"H", "[", 
       RowBox[{"L", "-", "z"}], "]"}]}], ")"}], 
    RowBox[{"(", "\[IndentingNewLine]", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"n", ">", "1"}], ",", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            RowBox[{"\[CapitalGamma]", "[", 
             RowBox[{"Z0", ",", "RS"}], "]"}], 
            RowBox[{"\[CapitalGamma]", "[", 
             RowBox[{"Z0", ",", "RL"}], "]"}]}], ")"}], 
          RowBox[{"n", "-", "1"}]], ",", "1"}], "]"}], 
       RowBox[{"V0", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"t", "-", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"n", "-", "1"}], ")"}], 
            RowBox[{"(", 
             RowBox[{"2", 
              FractionBox["L", "vp"]}], ")"}]}]}], ")"}], "-", 
