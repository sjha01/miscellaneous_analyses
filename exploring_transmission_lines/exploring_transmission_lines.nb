Cell[TextData[{
 "Below...\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"V\[LetterSpace]plus\[LetterSpace]line", "[", 
    RowBox[{"n", ",", "..."}], "]"}], TraditionalForm]],ExpressionUUID->
  "59546b36-1679-4f4a-999d-65ed9e9e11b4"],
 " is the source wave for n=1, the first wave reflected from the source for \
n=2, the second wave reflected from the source for n=3, etc. All of these are \
on the transmission line.\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"V\[LetterSpace]minus\[LetterSpace]line", "[", 
    RowBox[{"n", ",", "..."}], "]"}], TraditionalForm]],ExpressionUUID->
  "84a11354-7e00-41c5-8070-b2669bfa3906"],
 " is the first wave reflected from the load for n=1, the second wave \
reflected from the load for n=2, etc. All of these are on the transmission \
line.\n",
 Cell[BoxData[
  FormBox[
   RowBox[{"V\[LetterSpace]load", "[", 
    RowBox[{"n", ",", "..."}], "]"}], TraditionalForm]],ExpressionUUID->
  "184a53a9-6a46-4197-b6c2-64163ebb78b0"],
 " is the first wave transmitted through the load for n=1, the second wave \
transmitted through the load for n=2, etc. All of these are on the load.\nThe \
total solution on the line is therefore ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["V", "line"], "(", 
     RowBox[{"z", ",", "t"}], ")"}], "=", 
    RowBox[{
     SubscriptBox["\[Sum]", 
      RowBox[{"n", "=", "1"}]], 
     RowBox[{"[", 
      RowBox[{
       RowBox[{
        SuperscriptBox[
         SubscriptBox["V", "line"], "+"], "(", 
        RowBox[{"n", ",", "..."}], ")"}], "+", 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["V", "line"], "-"], "(", 
        RowBox[{"n", ",", "..."}], ")"}]}], "]"}]}]}], TraditionalForm]],
  ExpressionUUID->"79e9b714-395d-4d13-b22d-3387814769f2"],
 ", and the total solution for line and load combined is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"V", "(", 
     RowBox[{"z", ",", "t"}], ")"}], "=", 
    RowBox[{
     SubscriptBox["\[Sum]", 
      RowBox[{"n", "=", "1"}]], 
     RowBox[{"[", 
      RowBox[{
       RowBox[{
        SuperscriptBox[
         SubscriptBox["V", "line"], "+"], "(", 
        RowBox[{"n", ",", "..."}], ")"}], "+", 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["V", "line"], "-"], "(", 
        RowBox[{"n", ",", "..."}], ")"}], "+", 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["V", "load"], "+"], "(", 
        RowBox[{"n", ",", "..."}], ")"}]}], "]"}]}]}], TraditionalForm]],
  ExpressionUUID->"ad898ce3-7baa-4360-bd55-823d178614d1"],
 " So:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"V\[LetterSpace]total\[LetterSpace]line", "[", 
     RowBox[{"n", ",", "..."}], "]"}], "=", 
    RowBox[{
     RowBox[{"V\[LetterSpace]plus\[LetterSpace]line", "[", 
      RowBox[{"n", ",", "..."}], "]"}], "+", 
     RowBox[{"V\[LetterSpace]minus\[LetterSpace]line", "[", 
      RowBox[{"n", ",", "..."}], "]"}]}]}], TraditionalForm]],ExpressionUUID->
  "518774c1-d044-41ac-bd29-7936e6d00c05"],
 ".\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"V\[LetterSpace]total", "[", 
     RowBox[{"n", ",", "..."}], "]"}], "=", 
    RowBox[{
     RowBox[{"V\[LetterSpace]plus\[LetterSpace]line", "[", 
      RowBox[{"n", ",", "..."}], "]"}], "+", 
     RowBox[{"V\[LetterSpace]minus\[LetterSpace]line", "[", 
      RowBox[{"n", ",", "..."}], "]"}], "+", 
     RowBox[{"V\[LetterSpace]load", "[", 
      RowBox[{"n", ",", "..."}], "]"}]}]}], TraditionalForm]],ExpressionUUID->
  "1b139988-a103-4c6d-a054-ca2719f26584"]
}], "Text",
